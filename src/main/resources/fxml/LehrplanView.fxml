      <VBox spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label styleClass="headline" text="Lehrplan" />
            <HBox>
               <children>
                  <Button onAction="#fachAnlegen" text="Fach anlegen" />
               </children>
            </HBox>
            <TableView fx:id="faecherTable" VBox.vgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="fachColumn" prefWidth="400.0" text="Fach" />
                <TableColumn fx:id="semesterColumn" prefWidth="100.0" text="Semester" />
                  <TableColumn fx:id="noteColumn" prefWidth="75.0" text="Note" />
                  <TableColumn fx:id="ectsColumn" prefWidth="50.0" text="ECTS" />
              </columns>
            </TableView>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
