<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="10.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.hszg.notenprogramm.controller.LehrplanController">
   <children>
      <Label styleClass="headline" text="Lehrplan">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Label>
      <Button onAction="#fachAnlegen" text="Fach anlegen" />
      <TableView fx:id="faecherTable" VBox.vgrow="ALWAYS">
         <columns>
            <TableColumn fx:id="fachColumn" prefWidth="400.0" text="Fach" />
            <TableColumn fx:id="semesterColumn" prefWidth="100.0" text="Semester" />
            <TableColumn fx:id="noteColumn" prefWidth="75.0" text="Note" />
            <TableColumn fx:id="ectsColumn" prefWidth="50.0" text="ECTS" />
         </columns>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
